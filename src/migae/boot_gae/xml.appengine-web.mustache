<?xml version="1.0" encoding="UTF-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>{{app-id}}</application>
  <version>{{version}}</version>

  {{#appengine.thread-safe}}
  <threadsafe>true</threadsafe>
  {{/appengine.thread-safe}}

  {{#appengine.resource-files}}
  <resource-files>
    {{#include}}
    <include path="{{path}}"
	     {{#expiration}}expiration="{{expiration}}"
	     {{/expiration}}>
      {{#http-header}}
      <http-header name="{{name}}" value="{{value}}" />
      {{/http-header}}
    </include>
    {{/include}}
    {{#exclude}}
    <exclude path="{{path}}"
	     {{#expiration}}expiration="{{expiration}}"
	     {{/expiration}}>
      {{#http-header}}
      <http-header name="{{name}}" value="{{value}}" />
      {{/http-header}}
    </exclude>
    {{/exclude}}
  </resource-files>
  {{/appengine.resource-files}}

  {{#appengine.static-files}}
  <static-files>
    {{#include}}
    <include path="{{path}}"
	     {{#expiration}}expiration="{{expiration}}"
	     {{/expiration}}>
      {{#http-header}}
      <http-header name="{{name}}" value="{{value}}" />
      {{/http-header}}
    </include>
    {{/include}}
    {{#exclude}}
    <exclude path="{{path}}"
	     {{#expiration}}expiration="{{expiration}}"
	     {{/expiration}}>
      {{#http-header}}
      <http-header name="{{name}}" value="{{value}}" />
      {{/http-header}}
    </exclude>
    {{/exclude}}
  </static-files>
  {{/appengine.static-files}}

  {{#appengine.system-properties}}
  <system-properties>
    {{#props}}
    <property name="{{name}}"
	      value="{{value}}" />
    {{/props}}
    {{#appengine.logging.jul}}
    <property name="{{name}}"
	      value="{{value}}" />
    {{/appengine.logging.jul}}
    {{#appengine.logging.log4j}}
    <property name="{{name}}"
	      value="{{value}}" />
    {{/appengine.logging.log4j}}
  </system-properties>
  {{/appengine.system-properties}}


  <env-variables>
    {{#appengine.env-vars}}
    <env-var name="{{name}}" value="{{value}}" />
    {{/appengine.env-vars}}
  </env-variables>

  {{#appengine.ssl}}
  <ssl-enabled>true</ssl-enabled>
  {{/appengine.ssl}}

  {{#appstats.admin-console}}
  <admin-console>
    <page name="{{name}}" url="{{url}}" />
  </admin-console>
  {{/appstats.admin-console}}
</appengine-web-app>
